{
    "builds": [{
        "src": "myproject/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "myproject/wsgi.py"
        }
    ],
    "functions": {
        "myproject/wsgi.py": {
            "runtime": "python3.9"
        }
    }
}

